{
  "compileOnSave": true,
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": ["es5", "es6", "dom"],
    "module": "ES2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
